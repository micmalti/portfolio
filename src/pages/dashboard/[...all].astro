---
import { App } from "../../components/app";
import Layout from "../../layouts/Layout.astro";

export function getStaticPaths() {
  return [
    {params: {page: 'settings'}},
    {params: {page: 'profile'}},
  ];
}

const { page } = Astro.params;

---

<Layout Title={page}>
  <App client:only="react" />
</Layout>